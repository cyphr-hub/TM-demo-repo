pipeline {
   agent any
        environment { 
            CREDS = credentials('TMcreds') 
        }
        stages {
           stage ('TMcredsTest'){
               steps {
               
               sh('curl -u $CREDS_USR:$CREDS_PSW https://deloitte-poc.threatmodeler.net/')
               sh('echo $CREDS_USR:$CREDS_PSW') 
               }
             
           }
           
        }
}
